{
  "name": "godagent",
  "version": "2.0.0",
  "description": "God Agent - Self-Learning AI Orchestration System with 197 specialized agents, intelligent routing, and persistent memory",
  "author": "txc0ld",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/txc0ld/godagent.git"
  },
  "homepage": "https://github.com/txc0ld/godagent#readme",
  "bugs": {
    "url": "https://github.com/txc0ld/godagent/issues"
  },
  "keywords": [
    "ai",
    "agent",
    "llm",
    "claude",
    "anthropic",
    "orchestration",
    "machine-learning",
    "self-learning",
    "multi-agent",
    "memory-system"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "god-agent": "./dist/god-agent/cli.js",
    "phd-cli": "./dist/god-agent/cli/phd-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:benchmarks": "vitest bench",
    "benchmark": "node --expose-gc node_modules/.bin/vitest bench tests/benchmarks/vector-db.bench.ts",
    "benchmark:ci": "node --expose-gc node_modules/.bin/vitest bench tests/benchmarks/vector-db.bench.ts --reporter=verbose",
    "lint": "eslint src tests --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "demo": "npx tsx examples/god-agent-demo.ts",
    "start:ui": "npm run ui:install && concurrently -n api,web -c cyan,magenta \"npm run ui:api\" \"npm run ui:dev\"",
    "ui:install": "cd frontend && npm install",
    "ui:dev": "cd frontend && npm run dev",
    "ui:api": "cd frontend && npx tsx server/index.ts",
    "ui:build": "cd frontend && npm run build",
    "observe": "npx tsx src/god-agent/observability/daemon.ts",
    "observe:start": "npx tsx src/god-agent/observability/daemon.ts start",
    "observe:stop": "npx tsx src/god-agent/observability/daemon.ts stop",
    "observe:status": "npx tsx src/god-agent/observability/daemon.ts status",
    "observe:open": "npx tsx src/god-agent/observability/daemon.ts open",
    "memory:start": "npx tsx src/god-agent/core/memory-server/memory-daemon.ts start",
    "memory:stop": "npx tsx src/god-agent/core/memory-server/memory-daemon.ts stop",
    "memory:status": "npx tsx src/god-agent/core/memory-server/memory-daemon.ts status",
    "daemon:start": "npx tsx src/god-agent/core/daemon/daemon-cli.ts start",
    "daemon:stop": "npx tsx src/god-agent/core/daemon/daemon-cli.ts stop",
    "daemon:status": "npx tsx src/god-agent/core/daemon/daemon-cli.ts status",
    "ucm:start": "npx tsx src/god-agent/core/ucm/daemon/ucm-cli.ts start",
    "ucm:stop": "npx tsx src/god-agent/core/ucm/daemon/ucm-cli.ts stop",
    "ucm:status": "npx tsx src/god-agent/core/ucm/daemon/ucm-cli.ts status",
    "god-agent:start": "npm run memory:start && npm run daemon:start && npm run ucm:start && npm run observe:start && echo '✅ All God Agent services started'",
    "god-agent:stop": "npm run observe:stop && npm run ucm:stop && npm run daemon:stop && npm run memory:stop && echo '✅ All God Agent services stopped'",
    "god-agent:status": "echo '=== God Agent Service Status ===' && npm run memory:status && npm run daemon:status && npm run ucm:status && npm run observe:status"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "better-sqlite3": "^12.5.0",
    "commander": "^11.0.0",
    "concurrently": "^8.2.2",
    "express": "^5.2.1",
    "markdown-it": "^14.1.0",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.17.2",
    "proper-lockfile": "^4.1.2",
    "uuid": "^13.0.0",
    "yoga-wasm-web": "^0.3.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/express": "^5.0.6",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^27.0.0",
    "@types/lz4": "^0.6.4",
    "@types/node": "^20.10.0",
    "@types/proper-lockfile": "^4.1.4",
    "@types/shell-quote": "^1.7.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "js-yaml": "^4.1.1",
    "jsdom": "^27.3.0",
    "lz4": "^0.6.5",
    "marked": "^17.0.1",
    "playwright": "^1.57.0",
    "puppeteer-core": "^24.33.0",
    "shell-quote": "^1.8.3",
    "supertest": "^7.1.4",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
